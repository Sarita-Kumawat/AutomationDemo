package Pages;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;

public class ViewJobData 
{
 WebDriver driver;
 By standard = By.xpath("//a[text()='Standard Reports']");
 
  public ViewJobData(WebDriver driver)
  {
	  this.driver = driver;
  }
  
  public void click_Standard()
  {
	  driver.findElement(standard).click();
  }
}
