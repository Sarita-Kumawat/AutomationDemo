package Pages;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;

public class HomePage 
{
WebDriver driver;
By DataAn = By.xpath("//a[text()='Data Analysis']");

public HomePage(WebDriver driver)
{
this.driver =driver;	
}

public void click_DataAnalysis()
{
driver.findElement(DataAn).click();	
}
}
